@{
    Layout = null;
    bool canEdit = false;
}
<style>
    #calendar {
        width: 90%;
        margin: 0 auto;
    }

    .Assignment {
        background-color: #ffd800 !important;
        color: #fff !important;
    }

    .Holiday {
        background-color: palegreen !important;
        color: #fff !important;
    }

    .ClassTest {
        background-color: #ff6a00 !important;
        color: #fff !important;
    }

    body .modal {
        /* new custom width */
        width: 800px;
        /* must be half of the width, minus scrollbar on the left (30px) */
        margin-left: -400px;
    }

    #loading {
        display: none;
    }

    #dvLoading {
        background: url(../../images/ajax-loader.gif) no-repeat center center;
        height: 100px;
        width: 100px;
        position: fixed;
        z-index: 1000;
        left: 50%;
        top: 50%;
        margin: -25px 0 0 -25px;
    }

    #filelist div.Attachment {
        border: 1px solid #ccc;
        padding: 5px;
        margin-bottom: 2px;
    }

        #filelist div.Attachment a.deleteFile {
            float: right;
        }
</style>
<div id='calendar'></div>


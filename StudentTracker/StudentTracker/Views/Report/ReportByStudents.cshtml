@{
    ViewBag.Title = "ReportByStudents";
    Layout = "~/Views/Shared/_LayoutBlueMoon.cshtml";
}

<div class="sub-nav">
    <ul>
        <li>
            <a href="#">Home
            </a>
        </li>
    </ul>

</div>
<div class="dashboard-wrapper">
    <div class="left-sidebar">
        <div class="row-fluid">
            <div class="span12">
                <div class="widget no-margin">

                    <div class="widget-header">
                        <div class="title textAlign">
                            <div class="pull-left">Attendance Reports</div>
                            View By Student</div>
                        <span class="tools tls2"><a href="/Report" class="btn btn-info">Back</a> <a href="/Report/AssessmentCentre" class="btn btn-info">Cancel</a></span>
                    </div>

                    <div class="widget-body">
                        <div class="assigment_ctn">
                            <div class="assigment_box">
                                <div class="cal_select">
                                    <ul>
                                        <li>
                                            <span class="slt_txt">Filters</span>							</li>
                                        <li>
                                            <select id="id_select" class="selectpicker bla bla bli">
                                                <option>All Courses</option>
                                                <option>All Courses</option>
                                            </select></li>
                                        <li>
                                            <select id="id_select" class="selectpicker bla bla bli">
                                                <option>All Departments</option>
                                                <option>bull</option>
                                            </select></li>
                                        <li>
                                            <select id="id_select" class="selectpicker bla bla bli">
                                                <option>All Classes</option>
                                                <option>bull</option>
                                            </select></li>
                                        <li>
                                            <select name="select" class="selectpicker bla bla bli" id="select">
                                                <option>All Staff</option>
                                                <option>bull</option>
                                            </select>
                                        </li>
                                        <li>
                                            <select id="id_select" class="selectpicker bla bla bli">
                                                <option>All Section</option>
                                                <option>bull</option>
                                            </select></li>
                                        <li>
                                            <select id="id_select" class="selectpicker bla bla bli">
                                                <option>All Subject</option>
                                                <option>bull</option>
                                            </select></li>
                                    </ul>
                                </div>
                                <div class="cal_all">
                                    <ul>
                                        <li><span>Select Attendance type:</span></li>
                                        <li>
                                            <input type="radio" name="a" />
                                            <span>By Lecture</span>
                                        </li>
                                        <li>
                                            <input type="radio" name="a" />
                                            <span>By Day</span>
                                        </li>
                                    </ul>
                                </div>

                                <div class="cal_all2">
                                    <ul>
                                        <li><span>Custom Report</span></li>
                                        <li>
                                            <div class="input-append span10 date_picker">
                                                <input type="text" name="date_range1" id="date_range1" class="date_picker span12" placeholder="01/29/2013 - 01/31/2013" />
                                                <span class="add-on btn date_picker">
                                                    <i class="icon-calendar"></i>
                                                </span>
                                            </div>
                                        </li>
                                        <li>
                                            <a href="#" class="btn btn-info">Generate</a>
                                        </li>
                                    </ul>
                                </div>
                                <div class="cal_all2">
                                    <ul>
                                        <li><span>Student Name</span></li>
                                        <li>
                                            <input type="text" name="" id="" class="" placeholder="Search by student Name" />
                                        </li>
                                        <li>
                                            <a href="#" class="btn btn-info">Search</a>
                                        </li>
                                    </ul>
                                </div>

                                <div class="assigment_tbl">
                                    <table class="table table-striped table-bordered no-margin">
                                        <thead>
                                            <tr class="bluRow">

                                                <th>Roll No.
                                                </th>
                                                <th>Student Name
                                                </th>
                                                <th class="hidden-phone">Present
                                                </th>
                                                <th class="hidden-phone">Absent
                                                </th>
                                                <th class="hidden-phone">Leave
                                                </th>
                                                <th class="hidden-phone">Total 
                                                </th>
                                                <th class="hidden-phone">Comments From Teachers 
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="">
                                                <td>1.
                                                </td>
                                                <td>
                                                    <img src="img/student_name1.jpg" alt="">
                                                    <span class="blue_color">Adams, Ashley</span> </td>
                                                <td class="hidden-phone">15(50%)
                                                </td>
                                                <td class="hidden-phone">15(50%)
                                                </td>
                                                <td class="hidden-phone">0(0%)
                                                </td>

                                                <td class="hidden-phone">50%
                                                </td>
                                                <td class="hidden-phone">Not very Good
                                                </td>

                                            </tr>
                                        </tbody>
                                    </table>
                                </div>



                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div id="dhtmlgoodies_contentBox">
            <div id="dhtmlgoodies_content">
                <div class="row-fluid" style="margin-top: 20px;">
                    <div class="span9">
                        <div class="row-fluid">
                            <div class="left-sidebar2">
                                <div class="row-fluid">
                                    <div class="span12">
                                        <div class="widget no-margin">
                                            <div class="widget-header">
                                                <div class="title textAlign">
                                                    <div class="poLft2"><a href="testButton.html" class="btn btn-info">Test</a> <a href="worksheet.html" class="btn btn-info">Work Sheet</a></div>
                                                    TEST CENTRE
                                                </div>
                                                <span class="tools tls2"><a href="#" class="btn btn-info">Close all overdue</a> <a href="result.html" class="btn btn-info">Results</a></span>
                                            </div>
                                            <div class="widget-body">
                                                <div class="test_ctn">
                                                    <div class="row-fluid">
                                                        <div class="span12 test_ctnLft">
                                                            <div class="hdr_sec">
                                                                <h1>English <span>Class Test For Class 10/ABC</span></h1>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row-fluid">
                                                        <div class="span12 hdr_btn_sec">
                                                            <div class="hdr_btn_secInr">
                                                                <p>Mr. S. Singh set this class test on April 30, 2013</p>
                                                                <a href="#">
                                                                    <img src="img/time_icon5.png">Class test on Thu 2nd May</a>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row-fluid">
                                                        <div class="span12">
                                                            <div class="tex_sec">
                                                                <h3>English phrase</h3>
                                                                <p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only five centuries,</p>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>





                        </div>
                    </div>
                    <div class="span3">
                        <div class="right_side_bar2">
                            <div class="btnFild">
                                <a href="#" class="arrow_box">Close Submission</a>
                            </div>
                            <div class="btnFild">
                                <a href="#" class="arrow_box2">Print this homework</a>
                            </div>
                            <div class="btnFild">
                                <a href="#" class="arrow_box3">Re-use Class test</a>
                            </div>
                            <div class="btnFild">
                                <a href="#" class="arrow_box4">Edit Class Test</a>
                            </div>
                            <div class="btnFild">
                                <a href="#" class="arrow_box5">Delete Class Test</a>
                            </div>
                            <h3>Have a question<br>
                                Visit our Knowlwdge Base</h3>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="right-sidebar">
        @Html.Partial("_RightMenuPartial")
    </div>
</div>


@{
    Layout = null;
}

@model IList<StudentTracker.Core.Entities.Tag>

<div class="headline">
    <h3>Blog Tags</h3>
</div>
<ul class="unstyled inline blog-tags">

    @if (Model.Count > 0)
    {
        foreach (var tag in Model)
        {
        <li>
            <i class="icon-tags"></i>
            @Html.TagLink(tag)
        </li>
        }
    }
    else
    {
        <p>No tags found!</p>
    }
</ul>

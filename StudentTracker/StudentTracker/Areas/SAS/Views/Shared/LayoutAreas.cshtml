@{
    ViewBag.Title = "LayoutAreas";
}

<html>
<head>
    <title></title>
    <link href="/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="/Areas/SAS/assets/css/style.css" rel="stylesheet" />
    <link href="/Areas/SAS/assets/css/headers/header1.css" rel="stylesheet" />
    <link href="/Content/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="/Areas/SAS/assets/css/style_responsive.css" rel="stylesheet" />
    <link rel="shortcut icon" href="favicon.ico">
    <link href="/Areas/SAS/assets/plugins/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="/Areas/SAS/assets/plugins/flexslider/flexslider.css" rel="stylesheet" />
    <link rel="stylesheet" href="/Areas/SAS/assets/plugins/parallax-slider/css/parallax-slider.css" type="text/css">
    <link href="/Areas/SAS/assets/css/themes/default.css" id="style_color" rel="stylesheet" />
    <link rel="stylesheet" href="/Areas/SAS/assets/css/themes/headers/default.css" id="style_color-header-1">
    <link href="/Content/themes/base/jquery.ui.all.css" rel="stylesheet" />
</head>

<body>
    <!--=== Style Switcher ===-->
    <i class="style-switcher-btn icon-cogs"></i>
    <div class="style-switcher">
        <div class="theme-close"><i class="icon-remove"></i></div>
        <div class="theme-heading">Theme Colors</div>
        <ul class="unstyled">
            <li class="theme-default theme-active" data-style="default" data-header="light"></li>
            <li class="theme-blue" data-style="blue" data-header="light"></li>
            <li class="theme-orange" data-style="orange" data-header="light"></li>
            <li class="theme-red" data-style="red" data-header="light"></li>
            <li class="theme-light" data-style="light" data-header="light"></li>
        </ul>
    </div>
    <!--/style-switcher-->
    <!--=== End Style Switcher ===-->

    <!--=== Top ===-->
    <div class="top">
        <div class="container">
            @Html.Partial("_LoginPartialSAS")
        </div>
    </div>
    <!--/top-->
    <!--=== End Top ===-->

    <!--=== Header ===-->
    <div class="header">
        <div class="container">
            <!-- Logo -->
            <div class="logo">
                <a href="index.html">
                    <img id="logo-header" src="/Areas/SAS/assets/img/logo1-default.png" alt="Logo"></a>
            </div>
            <!-- /logo -->

            <!-- Menu -->
            <div class="navbar">
                <div class="navbar-inner">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>
                    <!-- /nav-collapse -->
                    <div class="nav-collapse collapse">
                        <ul class="nav top-2">
                            <li class="active lnkMenu" id="lnkHome">
                                <a href="/SAS/SASHome/">Home</a>
                            </li>
                            <li class="lnkMenu" id="lnkAboutUs">
                                <a href="/SAS/SASHome/AboutUs">ABOUT US </a>
                            </li>
                            <li class="lnkMenu" id="lnkProducts"><a href="" class="dropdown-toggle" data-toggle="dropdown">OUR PRODUCTS
                        	<b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="/SAS/SASHome/Communication">Communication</a></li>
                                    <li><a href="/SAS/SASHome/Academic">Academic</a></li>
                                    <li><a href="/SAS/SASHome/Payment">Payment</a></li>
                                </ul>
                                <b class="caret-out"></b>
                            </li>
                            <li class="lnkMenu" id="lnkBlog"><a href="/SAS/SASHome/Blog">BLOG</a></li>
                            <li class="lnkMenu" id="lnkSupport"><a href="/SAS/SASHome/Support">SUPPORT</a></li>
                            <li class="lnkMenu" id="lnkContact"><a href="/SAS/SASHome/ContactUs">CONTACT US </a></li>
                            <li><a class="search"><i class="icon-search search-btn"></i></a></li>
                        </ul>
                        <div class="search-open">
                            <div class="input-append">
                                <form>
                                    <input type="text" class="span3" style="height: 30px;" placeholder="Search" />
                                    <button type="submit" class="btn-u">Go</button>
                                </form>
                            </div>
                        </div>
                    </div>
                    <!-- /nav-collapse -->
                </div>
                <!-- /navbar-inner -->
            </div>
            <!-- /navbar -->
        </div>
        <!-- /container -->
    </div>
    <!--/header -->
    <!--=== End Header ===-->
    @RenderBody()

    <!--=== Footer ===-->
